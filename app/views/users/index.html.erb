<%= search_form_for @q do |f| %>
  <%= f.label :name_cont, "Name" %>
  <%= f.search_field :name_cont %>
  <%= f.label :age_eq, "Age" %>
  <%= f.search_field :age_eq %>
  <%= f.submit "Search" %>
<% end %>
<table>
  <tbody>
    <thead>
      <tr>
        <th>Name</th>
        <th>Age</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>
